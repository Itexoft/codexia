{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "window:default",
    "webview:default",
    "dialog:default",
    "menu:default",
    "tray:default",
    "shell:default",
    "fs:default",
    "fs:read-all",
    "path:default",
    "opener:default",
    "log:default",
    "store:default",
    "single-instance:default",
    "core:window:allow-start-dragging"
  ]
}
