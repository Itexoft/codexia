{
  "$schema": "../node_modules/@tauri-apps/cli/schema/capabilities.schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:webview:default",
    "dialog:default",
    "fs:default",
    "core:path:default",
    "opener:default",
    "log:default",
    "store:default",
    "core:window:allow-start-dragging",
    "shell:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "system-ssh",
          "cmd": "/usr/bin/ssh",
          "args": true,
          "sidecar": false
        }
      ]
    }
  ]
}
