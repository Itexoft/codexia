{
  "$schema": "../node_modules/@tauri-apps/cli/schema/capabilities.schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:webview:default",
    "dialog:default",
    "core:menu:default",
    "core:tray:default",
    "shell:default",
    "fs:default",
    "fs:read-all",
    "core:path:default",
    "opener:default",
    "log:default",
    "store:default",
    "core:window:allow-start-dragging"
  ]
}
